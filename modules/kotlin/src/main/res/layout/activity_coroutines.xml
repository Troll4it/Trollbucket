<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <com.google.android.material.button.MaterialButton        android:id="@+id/coroutines_start"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:text="协程启动"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <com.google.android.material.button.MaterialButton        android:id="@+id/coroutines_job"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:text="协程Job"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/coroutines_start" />    <com.google.android.material.button.MaterialButton        android:id="@+id/coroutines_channel"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:text="协程Channel"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/coroutines_job" />    <com.google.android.material.button.MaterialButton        android:id="@+id/coroutines_select"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:text="协程Select"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/coroutines_channel" />    <EditText        android:id="@+id/coroutines_et"        android:layout_width="match_parent"        android:layout_height="50dp"        app:layout_constraintTop_toBottomOf="@id/coroutines_select" />    <com.google.android.material.button.MaterialButton        android:id="@+id/coroutines_over_refresh"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:text="过度刷新"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/coroutines_et" /></androidx.constraintlayout.widget.ConstraintLayout>