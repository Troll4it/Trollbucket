<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#c10600">    <TextView        android:id="@+id/tv"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_marginStart="50dp"        android:layout_marginEnd="50dp"        android:background="@drawable/red_packet_process_button_bg"        android:gravity="center"        android:text="立即提现"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <ImageView        android:id="@+id/iv_circle"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginEnd="28dp"        android:src="@drawable/icon_click_circle_show"        app:layout_constraintBottom_toBottomOf="@id/tv"        app:layout_constraintEnd_toEndOf="@id/tv"        app:layout_constraintTop_toTopOf="@id/tv" />    <ImageView        android:id="@+id/iv_finger"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="15dp"        android:src="@drawable/red_packet_process_finger"        app:layout_constraintEnd_toEndOf="parent"        android:layout_marginEnd="48dp"        app:layout_constraintTop_toTopOf="@id/tv" /></androidx.constraintlayout.widget.ConstraintLayout>