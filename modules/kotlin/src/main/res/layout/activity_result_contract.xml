<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <com.google.android.material.button.MaterialButton        android:id="@+id/contract_button"        android:layout_width="match_parent"        android:layout_height="60dp"        android:text="start_Activity_For_Result"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <com.google.android.material.button.MaterialButton        android:id="@+id/take_picture"        android:layout_width="match_parent"        android:layout_height="60dp"        android:text="拍照"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/contract_button" />    <com.google.android.material.button.MaterialButton        android:id="@+id/permission_read"        android:layout_width="match_parent"        android:layout_height="60dp"        android:text="存储权限"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/take_picture" /></androidx.constraintlayout.widget.ConstraintLayout>