<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:ignore="MissingDefaultResource">    <ImageView        android:id="@+id/project_tv_back"        android:layout_width="40dp"        android:layout_height="40dp"        android:padding="4dp"        android:src="@mipmap/left"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <TextView        android:id="@+id/project_tv_title"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:padding="4dp"        android:text="项目"        android:textSize="18sp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <androidx.recyclerview.widget.RecyclerView        android:id="@+id/project_rv"        android:layout_width="match_parent"        android:layout_height="wrap_content"        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"        app:layout_constraintTop_toBottomOf="@id/project_tv_title" /></androidx.constraintlayout.widget.ConstraintLayout>